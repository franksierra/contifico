{
  "name": "franksierra/contifico",
  "description": "Use some Contifico APIs to manage customer, product and inventory.",
  "keywords": [
    "Contifico",
    "API",
    "SDK"
  ],
  "homepage": "https://github.com/franksierra/contifico",
  "license": "MIT",
  "authors": [
    {
      "name": "Frank Sierra",
      "email": "sierrafayad@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/franksierra/contifico",
    "issues": "https://github.com/franksierra/contifico/issues"
  },
  "require": {
    "php": ">=7.2 <8.2",
    "ext-json": "*",
    "apimatic/unirest-php": "^4.0.0",
    "apimatic/core-interfaces": "~0.1.0",
    "apimatic/core": "~0.1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phan/phan": "5.3.1",
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5"
  },
  "autoload": {
    "psr-4": {
      "Contifico\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Contifico\\Tests\\": "tests/"
    }
  }
}
